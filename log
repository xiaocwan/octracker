  -o, --output=''
  -p, --patch=''
      --record=false
      --type='strategic'

oc process
  -f, --filename=''
  -l, --labels=''
  -o, --output='json'
      --output-version=''
      --parameters=false
      --raw=false
  -t, --template=''
  -v, --value=[]

oc export
      --all=true
      --all-namespaces=false
      --as-template=''
      --exact=false
  -f, --filename=[]
      --no-headers=false
  -o, --output=''
      --output-version=''
      --raw=false
  -l, --selector=''
  -a, --show-all=true
      --show-labels=false
      --sort-by=''
  -t, --template=''

oc policy who-can
      --all-namespaces=false

oc policy add-role-to-user
      --role-namespace=''
  -z, --serviceaccount=[]

oc policy remove-role-from-user
      --role-namespace=''
  -z, --serviceaccount=[]

oc policy add-role-to-group
      --role-namespace=''

oc policy remove-role-from-group
      --role-namespace=''

